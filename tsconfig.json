{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "bazel-out/k8-fastbuild/bin/external/npm/node_modules/@types/*",
        "bazel-out/darwin-fastbuild/bin/external/npm/node_modules/@types/*",
        "bazel-out/x64_windows-fastbuild/bin/external/npm/node_modules/@types/*",
        "bazel-out/k8-dbg/bin/external/npm/node_modules/@types/*",
        "bazel-out/darwin-dbg/bin/external/npm/node_modules/@types/*",
        "bazel-out/x64_windows-dbg/bin/external/npm/node_modules/@types/*",
        "bazel-out/k8-fastbuild/bin/external/npm/node_modules/*",
        "bazel-out/darwin-fastbuild/bin/external/npm/node_modules/*",
        "bazel-out/x64_windows-fastbuild/bin/external/npm/node_modules/*",
        "bazel-out/k8-dbg/bin/external/npm/node_modules/*",
        "bazel-out/darwin-dbg/bin/external/npm/node_modules/*",
        "bazel-out/x64_windows-dbg/bin/external/npm/node_modules/*"
      ],
      "@usrbinboat/*": [
        "bazel-out/k8-fastbuild/bin/packages/*",
        "bazel-out/darwin-fastbuild/bin/packages/*",
        "bazel-out/x64_windows-fastbuild/bin/packages/*",
        "bazel-out/k8-dbg/bin/packages/*",
        "bazel-out/darwin-dbg/bin/packages/*",
        "bazel-out/x64_windows-dbg/bin/packages/*"
      ]
    },
    "rootDirs": [
      ".",
      "bazel-out/k8-fastbuild/bin",
      "bazel-out/darwin-fastbuild/bin",
      "bazel-out/x64_windows-fastbuild/bin",
      "bazel-out/k8-dbg/bin",
      "bazel-out/darwin-dbg/bin",
      "bazel-out/x64_windows-dbg/bin"
    ]
  },
  "exclude": ["bazel-*"]
}
